<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    >
<LinearLayout
    android:background="@color/white"
    android:layout_margin="10dp"
              android:orientation="vertical" android:layout_width="match_parent"
              android:layout_height="match_parent">
    <TextView
        android:textSize="18sp"
        android:id="@+id/history_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorPrimary"
        />

    <EditText
        android:id="@+id/et_searchData"
        android:hint="@string/sear_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <com.example.fazhao.locationmanager.baidu_map.widget.SwipeDeleteListView1
        android:id="@+id/list_history"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.example.fazhao.locationmanager.baidu_map.widget.SwipeDeleteListView1>
</LinearLayout>
    </FrameLayout>